{
    "name": "ca-web",
    "version": "0.1.0",
    "private": true,
    "dependencies": {
        "@testing-library/jest-dom": "^5.16.5",
        "@testing-library/react": "^13.4.0",
        "@testing-library/user-event": "^13.5.0",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-scripts": "5.0.1",
        "web-vitals": "^2.1.4"
    },
    "scripts": {
        "start": "react-scripts start",
        "build": "react-scripts build",
        "test": "react-scripts test",
        "eject": "react-scripts eject"
    },
    "eslintConfig": {
        "extends": [
            "react-app",
            "react-app/jest",
            "eslint:recommended"
        ],
        "rules": {
            "no-empty": "off",
            "no-debugger": "off",
            "no-unused-vars": "off",
            "semi": "warn",
            "curly": "warn",
            "eqeqeq": "warn",
            "no-new": "warn",
            "eol-last": "warn",
            "no-div-regex": "warn",
            "dot-notation": "warn",
            "require-await": "warn",
            "no-extra-semi": "warn",
            "no-extra-parens": "warn",
            "no-invalid-this": "warn",
            "no-useless-return": "warn",
            "quotes": [
                "warn",
                "single"
            ],
            "jsx-quotes": [
                "warn",
                "prefer-double"
            ],
            "comma-dangle": [
                "warn",
                "always-multiline"
            ],
            "arrow-parens": [
                "warn",
                "as-needed"
            ],
            "operator-linebreak": [
                "warn",
                "before"
            ],
            "indent": [
                "warn",
                4,
                {
                    "SwitchCase": 1
                }
            ]
        }
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    }
}